<!DOCTYPE html>
<html>
	<head>
		<meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1">
		<title>Edit Profile</title>
		<link rel="stylesheet" type="text/css" href="../stylesheets/default.css">
		<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
		<script src="/javascripts/pick_location.js"></script>
	</head>
	<body onload="initialize()">
		<div id="whole">
			<div id="naviBar">
				<form action="/edit_profile_move" method="POST" id="back">
					<div id="backBtn" style="cursor:pointer" onclick="$('#back').submit()"></div>
				</form>				
				<form action="/edit_profile_move" method="POST" id="done" name="done">
					<div id="doneBtn" style="cursor:pointer" onclick="insertLocation()"></div>
				</form>				
				<div id="profileNav"></div>
			</div>
			<div id="blankWithoutTab"></div>
			<div id="map_canvas" style="width: 100%; height:100%"></div>
		</div>
		<script src="http://code.jquery.com/jquery-1.10.1.min.js"></script>
		<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js"></script>
		<script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.9.2/jquery-ui.min.js"></script>
		<script src="http://code.jquery.com/jquery.js"></script>
		<script src="/lib/jquery.form.js"></script>
	</body>
</html>