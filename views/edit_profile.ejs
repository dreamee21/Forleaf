<!DOCTYPE html>
<html>
	<head>
		<meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1">
		<title>Edit Profile</title>
		<link rel="stylesheet" type="text/css" href="../stylesheets/default.css">
	</head>
	<body>
		<div id="whole">
			<div id="naviBar">
				<form action="/profile_move" method="POST" id="back">
					<div id="backBtn" style="cursor:pointer" onclick="$('#back').submit()"></div>
				</form>				
				<form action="/edit_profile_move" method="POST" id="edit">
					<div id="editBtn" style="cursor:pointer" onclick="insertProfile()"></div>
				</form>				
				<div id="profileNav"></div>
			</div>
			<div id="blankWithoutTab"></div>
			<div id="profilePictureWrapper_append">
			</div>
			<form action="/insert_profile" method="POST" name="insert_profile">	
				<div id="profileDetailWrapper">
				</div>
			</form>
		</div>	
		<script src="http://code.jquery.com/jquery-1.10.1.min.js"></script>
		<script src="/javascripts/edit_profile.js"></script>
		<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js"></script>
		<script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.9.2/jquery-ui.min.js"></script>
		<script src="http://code.jquery.com/jquery.js"></script>
		<script src="/lib/jquery.form.js"></script>
	</body>
</html>